{
    "env": {
        "browser": true,
        "jest/globals": true
    },
    "extends": "airbnb",
    "plugins": [
        "babel",
        "eslint-plugin-import",
        "eslint-plugin-react",
        "jest",
        "jsx-a11y"
    ],
    "rules": {
        "guard-for-in": 1,
        "lines-between-class-members": 0,
        "max-len": 0,
        "no-bitwise": 0,
        "no-confusing-arrow": 0,
        "no-console": 0,
        "no-else-return": 0,
        "no-multi-spaces": 0,
        "no-param-reassign": 0,
        "no-plusplus": 0,
        "no-return-assign": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": 1,
        "no-restricted-syntax": 1,
        "prefer-destructuring": 0,
        "spaced-comment": 0
    }
}
